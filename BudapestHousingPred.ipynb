    "#budapest_houses_selected[\"construction_year\"] = budapest_houses_selected[\"construction_year\"].apply(lambda x : Construction_Year_Parse(x))\n",
    "budapest_houses_selected[\"balcony\"].fillna(0,inplace=True)\n",
