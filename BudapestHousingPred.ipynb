   "source": [
    "HAS_ENCODING_FOR_AC = {\n",
    "    \"van\" : True,\n",
    "    \"nincs\" : False,\n",
    "    \"|nincs megadva|\" : False\n",
    "    }\n",
    "HAS_ENCODING_FOR_ELEVATOR = {\n",
    "    \"van\" : True,\n",
    "    \"nincs\" : False\n",
    "    }    \n",
    "\n",
    "\n",
    "def Floor_Parse(x):\n",
    "    if \"földszint\" in str(x):\n",
    "        return 0\n",
    "    else:\n",
    "        try:\n",
    "            return float(x)\n",
    "        except:\n",
    "            pass\n",
    "def Construction_Year_Parse(x):\n",
    "    x = str(x).split()\n",
    "    if len(x) > 2:\n",
    "        try:\n",
    "            return (float(x[0]) + float(x[2])) / 2\n",
    "        except:\n",
    "            pass\n",
    "    elif len(x) > 1: \n",
    "        try:\n",
    "            return float(x[0])\n",
    "        except:\n",
    "            pass\n",
    "    elif len(x) == 1:\n",
    "        try:\n",
    "            return float(x[0])\n",
    "        except:\n",
    "            pass\n",
    "def adress_parse_to_district(x):\n",
    "    x = str(x).split()\n",
    "    try:\n",
    "        return x[0]\n",
    "    except:\n",
    "        pass\n"
   ]
